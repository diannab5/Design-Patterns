package command;

public class ComandaInternare {
	private Medic medic;
	private String numePacient;
	
	
	public ComandaTratare(Medic medic, String numePacient) {
		super();
		this.medic = medic;
		this.numePacient = numePacient;
	}


	@Override
	public void executaComanda() {
		medic.internare(numePacient);
		
	}
}

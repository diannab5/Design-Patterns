package simplefactory;

public class Atacant implements Jucator {
	  private String name;
	     
	     
		public String getName() {
			return name;
		}

		public void setName(String name) {
			this.name = name;
		}
	@Override
	public String getNamePlayer() {
		return "Atacant";
	}

	@Override
	public String getPosition() {
		return null;
	}

}

package memento;

//daca aveam mm atribute salvam in memento doar pret fiindca asta se modifica
public class PachetTuristic {
	private double pretPachet;

	public PachetTuristic(double pretPachet) {
		super();
		this.pretPachet = pretPachet;
	}
	public double getPretPachet() {
		return pretPachet;
	}
}

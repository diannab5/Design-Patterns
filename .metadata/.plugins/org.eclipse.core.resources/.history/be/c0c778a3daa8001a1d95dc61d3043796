package simplefactory;

public class Fundas implements Jucator {
     private String name;
     
     
	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	@Override
	public String getNamePlayer() {
		return "Jucator";
	}

	@Override
	public String getPosition() {
		return null;
	}

}
